<h1>All Wikis</h1>
<div class="row">
  <div class="col-md-8">
      <% @wikis.each do |wiki| %>
          <div class="media">
            <div class="media-body">
              <h4 class="media-heading">
                  <p><%= link_to wiki.title, wiki_path(wiki.id) %></p>
              </h4>
            </div>
          </div>
      <% end %>
      <% if !user_signed_in? %>
        <p>Please sign up or sign in to see wikis</p>
      <%end%>
  </div>
  <div class="col-md-4">
    <% if user_signed_in? %>
      <%= link_to "Create a wiki", new_wiki_path, class: 'btn btn-success' %>
    <%end%>
  </div>
</div>
